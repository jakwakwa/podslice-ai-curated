{
    "name": "ai-podcast-curator",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "install:legacy": "npm install --legacy-peer-deps",
        "install:fast": "npm install --legacy-peer-deps",
        "install:clean": "rm -rf node_modules pnpm-lock.yaml package-lock.json && npm install --legacy-peer-deps",
        "install:diagnose": "node install-fix.mjs",
        "deps:cleanup": "node install-fix.mjs cleanup",
        "deps:analyze": "node analyze.mjs",
        "switch:pnpm": "rm -rf package-lock.json && pnpm install --no-frozen-lockfile",
        "dev": "npx prisma generate --no-engine && next dev",
        "dev:turbo": "npx prisma generate --no-engine && next dev --turbopack",
        "dev:legacy": "npx prisma generate --no-engine && npx @tailwindcss/cli -i ./app/globals.css -o ./dist/styles.css --watch & next dev --turbo",
        "build": "next build",
        "build:fast": "NODE_OPTIONS='--max-old-space-size=8192' SKIP_ENV_VALIDATION=true next build",
        "prisma:generate": "prisma generate --no-engine",
        "prisma:push": "npx prisma db push",
        "lint": "biome lint .",
        "lint:fix": "biome lint --write .",
        "start": "next start",
        "seed": "tsx scripts/seed.mjs",
        "seed:personas": "tsx scripts/seed-test-users.ts",
        "inngest": "inngest dev",
        "format": "biome format --write .",
        "format:fix": "biome format --write . && biome lint --write .",
        "caching": "ts-node ./src/caching.ts",
        "db:test:up": "docker compose -f docker-compose.test.yml up -d",
        "db:test:down": "docker compose -f docker-compose.test.yml down -v",
        "test:db:deploy": "bash -lc 'set -a; [ -f .env.test ] && source .env.test; set +a; DATABASE_URL=${TEST_POSTGRES_PRISMA_URL:-$TEST_DATABASE_URL} pnpm prisma migrate deploy'",
        "test": "vitest run",
        "test:watch": "vitest --watch",
        "test:ci": "vitest run --coverage"
    },
    "dependencies": {
        "@ai-sdk/google": "latest",
        "@clerk/nextjs": "^6.30.0",
        "@clerk/themes": "^2.4.6",
        "@clerk/types": "^4.74.0",
        "@elevenlabs/elevenlabs-js": "latest",
        "@google-cloud/storage": "^7.16.0",
        "@google/genai": "^1.13.0",
        "@iconscout/react-unicons": "2.2.3-rc.6ef815a5",
        "@paddle/paddle-js": "^1.4.2",
        "@paddle/paddle-node-sdk": "^3.2.0",
        "@prisma/client": "^6.13.0",
        "@prisma/extension-accelerate": "^2.0.2",
        "@radix-ui/react-alert-dialog": "^1.1.14",
        "@radix-ui/react-avatar": "^1.1.10",
        "@radix-ui/react-checkbox": "^1.3.2",
        "@radix-ui/react-collapsible": "^1.1.11",
        "@radix-ui/react-dialog": "^1.1.14",
        "@radix-ui/react-dropdown-menu": "^2.1.15",
        "@radix-ui/react-label": "^2.1.7",
        "@radix-ui/react-select": "^2.2.5",
        "@radix-ui/react-separator": "^1.1.7",
        "@radix-ui/react-slot": "^1.2.3",
        "@radix-ui/react-tabs": "^1.1.12",
        "@radix-ui/react-tooltip": "^1.2.7",
        "@stripe/stripe-js": "^7.8.0",
        "@tabler/icons-react": "^3.34.1",
        "@tailwindcss/postcss": "^4.1.11",
        "ai": "latest",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "date-fns": "4.1.0",
        "framer-motion": "^12.23.12",
        "inngest": "^3.40.1",
        "lucide-react": "^0.454.0",
        "mime": "^4.0.7",
        "next": "15.2.4",
        "next-themes": "^0.4.6",
        "nodemailer": "^6.10.1",
        "postcss-utilities": "^0.8.4",
        "react": "^19.1.1",
        "react-dom": "^19.1.1",
        "react-hook-form": "^7.62.0",
        "sonner": "^1.7.4",
        "stripe": "^18.4.0",
        "tailwind-merge": "^3.3.1",
        "uuid": "^11.1.0",
        "youtube-transcript": "^1.2.1",
        "zod": "^3.25.76",
        "zustand": "^5.0.7"
    },
    "devDependencies": {
        "@biomejs/biome": "2.1.2",
        "@csstools/postcss-cascade-layers": "^5.0.2",
        "@tailwindcss/cli": "^4.1.11",
        "@tailwindcss/line-clamp": "^0.4.4",
        "@tailwindcss/typography": "^0.5.16",
        "@tailwindcss/upgrade": "^4.1.11",
        "@testing-library/jest-dom": "^6.6.4",
        "@testing-library/react": "^16.3.0",
        "@types/node": "^22.15.32",
        "@types/nodemailer": "^6.4.17",
        "@types/react": "^19.1.9",
        "@types/react-dom": "^19.1.7",
        "autoprefixer": "^10.4.21",
        "cross-env": "^7.0.3",
        "dotenv": "^16.6.1",
        "jsdom": "^25.0.1",
        "knip": "^5.62.0",
        "postcss": "^8.5.6",
        "postcss-import": "^16.1.1",
        "postcss-mixins": "^12.1.2",
        "postcss-nested": "^7.0.2",
        "prisma": "6.11.1",
        "supertest": "^7.1.4",
        "tailwindcss": "^4.1.11",
        "ts-node": "10.9.2",
        "tsx": "^4.20.3",
        "tw-animate-css": "^1.3.6",
        "typescript": "5.8.2",
        "vite-tsconfig-paths": "^5.1.4",
        "vitest": "^2.1.9",
        "vitest-mock-extended": "^2.0.2"
    },
    "engines": {
        "node": "22.18.0"
    },
    "pnpm": {
        "overrides": {
            "mdns2": "npm:@empty-npm-package/empty-npm-package@1.0.0"
        },
        "onlyBuiltDependencies": [
            "@biomejs/biome",
            "@clerk/shared",
            "@parcel/watcher",
            "@prisma/client",
            "@prisma/engines",
            "@tailwindcss/oxide",
            "core-js",
            "esbuild",
            "oxc-resolver",
            "prisma",
            "protobufjs",
            "sharp",
            "tree-sitter",
            "tree-sitter-javascript",
            "tree-sitter-typescript"
        ]
    }
}
