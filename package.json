{
	"name": "ai-podcast-curator",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"install:legacy": "npm install --legacy-peer-deps",
		"install:fast": "npm install --legacy-peer-deps",
		"install:clean": "rm -rf node_modules pnpm-lock.yaml package-lock.json && npm install --legacy-peer-deps",
		"install:diagnose": "node install-fix.mjs",
		"deps:cleanup": "node install-fix.mjs cleanup",
		"deps:analyze": "node analyze.mjs",
		"switch:pnpm": "rm -rf package-lock.json && pnpm install --no-frozen-lockfile",
		"dev": "npx prisma generate --no-engine && next dev",
		"dev:turbo": "npx prisma generate --no-engine && next dev --turbopack",
		"dev:legacy": "npx prisma generate --no-engine && npx @tailwindcss/cli -i ./app/globals.css -o ./dist/styles.css --watch & next dev --turbo",
		"build": "next build",
		"build:fast": "NODE_OPTIONS='--max-old-space-size=8192' SKIP_ENV_VALIDATION=true next build",
		"prisma:generate": "prisma generate --no-engine",
		"prisma:push": "npx prisma db push",
		"prisma:migrate": "npx prisma migrate dev",
		"lint": "biome lint .",
		"lint:fix": "biome lint --write .",
		"start": "next start",
		"seed": "tsx scripts/seed.mjs",
		"seed:personas": "tsx scripts/seed-test-users.ts",
		"inngest": "inngest dev",
		"format": "biome format --write .",
		"format:fix": "biome format --write . && biome lint --write .",
		"caching": "ts-node ./src/caching.ts",
		"db:test:up": "docker compose -f docker-compose.test.yml up -d",
		"db:test:down": "docker compose -f docker-compose.test.yml down -v",
		"test:db:deploy": "bash -lc 'set -a; [ -f .env.test ] && source .env.test; set +a; DATABASE_URL=${TEST_POSTGRES_PRISMA_URL:-$TEST_DATABASE_URL} pnpm prisma migrate deploy'",
		"test": "vitest run",
		"test:watch": "vitest --watch",
		"test:ci": "vitest run --coverage"
	},
