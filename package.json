{
	"name": "ai-podcast-curator",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"install:legacy": "npm install --legacy-peer-deps",
		"install:fast": "npm install --legacy-peer-deps",
		"install:clean": "rm -rf node_modules pnpm-lock.yaml package-lock.json && npm install --legacy-peer-deps",
		"install:diagnose": "node install-fix.mjs",
		"deps:cleanup": "node install-fix.mjs cleanup",
		"deps:analyze": "node analyze.mjs",
		"switch:pnpm": "rm -rf package-lock.json && pnpm install --no-frozen-lockfile",
		"dev": "next dev --turbo",
		"dev:debug": "NODE_OPTIONS='--inspect' next dev --turbo",
		"dev:debug-fast": "NODE_OPTIONS='--inspect --max-old-space-size=6144' next dev --turbo",
		"dev:fast": "NODE_OPTIONS='--max-old-space-size=6144' next dev --turbo",
		"build": "npx prisma generate && NODE_OPTIONS='--max-old-space-size=8192' next build",
		"build:fast": "npx prisma generate && NODE_OPTIONS='--max-old-space-size=8192' SKIP_ENV_VALIDATION=true next build",
		"postinstall": "npx prisma generate",
		"prisma:generate": "npx prisma generate",
		"prisma:push": "npx prisma db push",
		"reset-db": "rm -rf prisma/migrations && npx prisma migrate dev --name init && npx prisma migrate reset --force",
		"lint": "npx @biomejs/biome lint .",
		"start": "next start",
		"seed": "tsx scripts/seed.mjs",
		"inngest": "inngest dev",
		"format": "npx @biomejs/biome format --write .",
		"caching": "ts-node ./src/caching.ts",
		"knip": "knip"
	},
	"dependencies": {
		"@ai-sdk/google": "latest",
		"@clerk/nextjs": "^6.25.4",
		"@clerk/themes": "^2.3.3",
		"@elevenlabs/elevenlabs-js": "latest",
		"@google-cloud/storage": "^7.16.0",
		"@iconscout/react-unicons": "^2.2.5",
		"@prisma/client": "^6.12.0",
		"@radix-ui/react-avatar": "^1.1.10",
		"@radix-ui/react-checkbox": "^1.3.2",
		"@radix-ui/react-collapsible": "^1.1.11",
		"@radix-ui/react-dialog": "^1.1.14",
		"@radix-ui/react-dropdown-menu": "^2.1.15",
		"@radix-ui/react-label": "^2.1.7",
		"@radix-ui/react-select": "^2.2.5",
		"@radix-ui/react-separator": "^1.1.7",
		"@radix-ui/react-slot": "^1.2.3",
		"@radix-ui/react-tabs": "^1.1.12",
		"@radix-ui/react-tooltip": "^1.2.7",
		"ai": "latest",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"framer-motion": "^12.23.6",
		"lucide-react": "^0.454.0",
		"next": "15.2.4",
		"next-themes": "^0.4.6",
		"nodemailer": "^6.10.1",
		"react": "^19.1.0",
		"react-dom": "^19.1.0",
		"sonner": "^1.7.4",
		"stripe": "^18.3.0",
		"tailwind-merge": "^2.6.0",
		"tailwindcss-animate": "^1.0.7",
		"youtube-transcript": "^1.2.1",
		"zod": "^3.25.76",
		"zustand": "^5.0.6"
	},
	"devDependencies": {
		"@biomejs/biome": "2.1.2",
		"@types/node": "22.15.32",
		"@types/nodemailer": "^6.4.17",
		"@types/react": "^19.1.8",
		"@types/react-dom": "^19.1.6",
		"inngest": "^3.40.1",
		"knip": "^5.62.0",
		"postcss": "^8.5.6",
		"prisma": "6.12.0",
		"tailwindcss": "^3.4.17",
		"ts-node": "10.9.2",
		"tsx": "^4.20.3",
		"typescript": "5.8.2"
	},
	"engines": {
		"node": "22.x"
	},
	"pnpm": {
		"overrides": {
			"mdns2": "npm:@empty-npm-package/empty-npm-package@1.0.0"
		}
	}
}
