{
	"editor.defaultFormatter": "biomejs.biome",
	"editor.formatOnSave": true,
	"[typescript]": {
		"editor.defaultFormatter": "biomejs.biome",
		"editor.codeActionsOnSave": {
			"quickfix.biome": "explicit",
			"source.organizeImports.biome": "explicit",
			"source.fixAll.biome": "explicit"
		}
	},
	"[typescriptreact]": {
		"editor.defaultFormatter": "vscode.typescript-language-features",
		"editor.codeActionsOnSave": {
			"quickfix.biome": "explicit",
			"source.organizeImports.biome": "explicit",
			"source.fixAll.biome": "explicit"
		}
	},
	"[javascript]": {
		"editor.defaultFormatter": "biomejs.biome",
		"editor.codeActionsOnSave": {
			"quickfix.biome": "explicit",
			"source.organizeImports.biome": "explicit",
			"source.fixAll.biome": "explicit"
		}
	},
	"[javascriptreact]": {
		"editor.defaultFormatter": "biomejs.biome",
		"editor.codeActionsOnSave": {
			"quickfix.biome": "explicit",
			"source.organizeImports.biome": "explicit",
			"source.fixAll.biome": "explicit"
		}
	},
	"terminal.integrated.fontFamily": "\"MesloLGS NF\", Meslo, mono",
	"workbench.preferredHighContrastColorTheme": "Vira Ocean High Contrast",
	"viraTheme.contrastedTabs": true,
	"workbench.preferredDarkColorTheme": "Vira Carbon High Contrast",
	"workbench.colorTheme": "Vira Carbon High Contrast",
	"workbench.preferredLightColorTheme": "Vira Palenight High Contrast",
	"workbench.preferredHighContrastLightColorTheme": "Vira Palenight High Contrast",
	"terminal.integrated.minimumContrastRatio": 1,
	"window.autoDetectColorScheme": true,
	"task.autoDetect": "off",
	"gradle.autoDetect": "off",
	"workbench.fontAliasing": "default",
	"workbench.activityBar.orientation": "vertical",
	"biome.enabled": true,
	"biome.configurationPath": "biome.json",
	"[json]": {
		"editor.defaultFormatter": "vscode.json-language-features"
	},
	"prisma.scriptRunner": "pnpm",
	"editor.foldingHighlight": false,
	"editor.guides.highlightActiveBracketPair": false,
	"editor.guides.highlightActiveIndentation": false,
	"editor.occurrencesHighlight": "off",
	"editor.renderLineHighlight": "gutter",
	"editor.selectionHighlight": false,
	"editor.renderLineHighlightOnlyWhenFocus": false,
	"editor.semanticHighlighting.enabled": "configuredByTheme",
	"editor.inlineSuggest.syntaxHighlightingEnabled": false,
	"gitlens.blame.highlight.enabled": false,
	"workbench.editor.highlightModifiedTabs": false,
	"editor.occurrencesHighlightDelay": 300,
	"gitlens.graph.highlightRowsOnRefHover": false,
	"editor.unicodeHighlight.invisibleCharacters": true,
	"editor.matchBrackets": "near",
	"editor.bracketPairColorization.enabled": false,
	"editor.copyWithSyntaxHighlighting": false,
	"viraTheme.showBorders": true,
	"viraTheme.solidLineHighlight": true,
	"viraTheme.hidesShadows": false,
	"editor.foldingImportsByDefault": true,
	"editor.tabSize": 2,
	"editor.codeLensFontFamily": "Menlo, Monaco, 'Courier New', monospace",
	"editor.codeLensFontSize": 8,
	"editor.inlayHints.fontFamily": "Menlo, Monaco, 'Courier New', monospace",
	"editor.inlayHints.fontSize": 4,
	"editor.fontLigatures": true,
	"terminal.integrated.fontLigatures.enabled": true,
	"editor.fontVariations": true,
	"editor.wordWrap": "wordWrapColumn",
}